CREATE TABLE Empleados_1 (
    ID SERIAL PRIMARY KEY,
    Nombre VARCHAR(50),
    Apellido VARCHAR(50),
    Telefono int NOT NULL,
    Direccion VARCHAR(100)
);

select nombre, apellido, telefono,direccion from Empleados_1
	
select * from Empleados_1 where direccion ='En mi corazón'
	
